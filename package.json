{
    "name": "graphene-pk11",
    "version": "2.0.9",
    "description": "A simple layer for interacting with PKCS #11 / PKCS11 / CryptoKI for Node in TypeScript",
    "main": "./build/graphene.js",
    "scripts": {
	"install": "tsd install && tsc",
	"test": "mocha",
	"pub": "npm install && npm version patch && npm publish && git push",
	"sync": "git ac && git pull --rebase && git push"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PeculiarVentures/graphene.git"
    },
    "engines": {
        "node": "^4.0.0"
    },
    "bin": {
        "graphene": "./build/console/console.js"
    },
    "dependencies": {
        "pkcs11js": "^1.0.0"
    },
    "keywords": [
        "pkcs11",
        "cryptography",
        "cryptoki",
        "token",
        "smartcard",
        "hsm",
